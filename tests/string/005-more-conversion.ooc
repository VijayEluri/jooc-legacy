
main: func {
    
    int1: Int = 100
    int1 toString() println()
    int1 as String println()
    
    long1: Long = 76
    long1 toString() println()
    long1 as String println()
    
    double1: Double = 10.0
    double1 toString() println()
    double1 as String println()
    
    char1: Char = 'a'
    char1 toString() println()
    char1 as String println()
    
}

